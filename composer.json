{
  "name":"contao-estatemanager/energy-pass",
  "type":"contao-bundle",
  "description":"Energy-Pass Extension for Contao EstateManager",
  "keywords":["contao", "immobilien", "manager", "property", "realestate", "energy", "certificate"],
  "homepage":"https://www.contao-estatemanager.com/",
  "license":"proprietary",
  "authors":[
    {
      "name":"Daniele Sciannimanica",
      "homepage":"https://github.com/doishub",
      "role":"Developer"
    }
  ],
  "require":{
    "php":"^7.4 || ^8.0",
    "contao/core-bundle":"^4.4",
    "contao-estatemanager/core": "^1.0"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.0",
    "contao/easy-coding-standard": "^3.4"
  },
  "conflict": {
    "contao/core": "*",
    "contao/manager-plugin": "<2.0 || >=3.0"
  },
  "autoload":{
    "psr-4": {
      "ContaoEstateManager\\EnergyPass\\": "src/"
    },
    "classmap": [
      "src/Resources/contao/"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config/",
      "src/Resources/contao/dca/",
      "src/Resources/contao/languages/",
      "src/Resources/contao/templates/"
    ]
  },
  "extra":{
    "contao-manager-plugin": "ContaoEstateManager\\EnergyPass\\ContaoManager\\Plugin"
  },
  "scripts": {
    "cs-fixer": [
      "vendor/bin/ecs check src/ --fix --ansi"
    ]
  }
}
